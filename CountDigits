#include <iostream>
using namespace std;
int Count(int a){

    if (a==0)
    {
        return 1;
    }
    
    int cnt = 0;
    while(a > 0){
        a = a / 10;
        cnt ++;
    }
    return cnt;
}
int main()
{
    int number;
    cin >> number;
    cout << Count(number) << endl;
    
    return 0;
}
